<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- styleable attributes for blurring view -->
    <declare-styleable name="BlurringView">
        <attr name="blurRadius" format="integer" />
        <attr name="downsampleFactor" format="integer" />
        <attr name="overlayColor" format="color" />
    </declare-styleable>

    <!--Scrim insets vie that allows the drawer the be behind the status bar and visible -->
    <declare-styleable name="ScrimInsetsFrameLayout">
        <attr name="insetForeground" format="reference|color" />
    </declare-styleable>


</resources>